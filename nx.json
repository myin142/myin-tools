{
  "npmScope": "nx-plug",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "aws-cdk": {
      "tags": []
    },
    "aws-cdk-e2e": {
      "tags": [],
      "implicitDependencies": ["aws-cdk"]
    },
    "aws-lambda": {
      "tags": []
    },
    "aws-lambda-e2e": {
      "tags": [],
      "implicitDependencies": ["aws-lambda"]
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  }
}
