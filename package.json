{
    "name": "myin-tools",
    "version": "0.0.0",
    "license": "MIT",
    "description": "Nx Plugin for AWS CDK",
    "scripts": {
        "nx": "nx",
        "start": "nx serve",
        "build": "nx run-many --target=build --all",
        "publish": "nx run-many --target=publish --all",
        "test": "nx run-many --target=test --all",
        "lint": "nx run-many --target=lint --all",
        "e2e": "nx run-many --target=e2e --all",
        "cli:build": "nx build cli --with-deps && npm i --prefix dist/packages/cli",
        "cli:publish": "node dist/packages/cli publishPackage"
    },
    "dependencies": {
        "@aws-cdk/core": "^1.54.0",
        "aws-cdk": "^1.54.0",
        "fs-extra": "^9.0.1",
        "yargs": "^15.4.1"
    },
    "devDependencies": {
        "@aws-cdk/assert": "^1.54.0",
        "@myin/cli": "0.0.8",
        "@myin/nx-builder": "0.0.2",
        "@nrwl/eslint-plugin-nx": "^10.0.2",
        "@nrwl/jest": "^10.0.2",
        "@nrwl/node": "^10.0.7",
        "@nrwl/nx-plugin": "^10.0.2",
        "@nrwl/workspace": "^10.0.2",
        "@types/fs-extra": "^9.0.1",
        "@types/jest": "25.1.4",
        "@types/node": "~8.9.4",
        "@types/npm": "^2.0.31",
        "@types/yargs": "^15.0.5",
        "@typescript-eslint/eslint-plugin": "2.19.2",
        "@typescript-eslint/parser": "2.19.2",
        "dotenv": "6.2.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.0.0",
        "jest": "25.2.3",
        "jest-extended": "^0.11.5",
        "jest-mock-extended": "^1.0.9",
        "jsii": "^1.10.0",
        "jsii-pacmak": "^1.10.0",
        "npm": "^6.14.7",
        "prettier": "2.0.4",
        "rollup": "^2.23.0",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-typescript2": "^0.27.1",
        "ts-jest": "25.2.1",
        "ts-node": "~7.0.0",
        "tslib": "^2.0.0",
        "tslint": "~6.0.0",
        "typescript": "~3.8.3"
    }
}
