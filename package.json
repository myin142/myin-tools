{
  "name": "myin-tools",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Nx Plugin for AWS CDK",
  "scripts": {
    "publish": "nx affected --target=publish",
    "build": "nx affected:build --with-deps",
    "postbuild": "if [[ -d dist/packages/cli ]]; then npm i --prefix dist/packages/cli; fi",
    "test": "nx affected:test",
    "lint": "nx affected:lint",
    "e2e": "nx affected:e2e",
    "cli:build": "nx build cli --with-deps && npm i --prefix dist/packages/cli",
    "cli:publish": "node dist/packages/cli publishPackage"
  },
  "dependencies": {
    "@aws-cdk/core": "^1.54.0",
    "aws-cdk": "^1.54.0",
    "aws-sdk": "^2.733.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@angular-devkit/core": "^13.2.3",
    "@angular-devkit/schematics": "^13.2.3",
    "@aws-cdk/assert": "^1.54.0",
    "@myin/cli": "0.0.8",
    "@myin/nx-builder": "0.0.2",
    "@nrwl/cli": "13.8.1",
    "@nrwl/eslint-plugin-nx": "13.8.1",
    "@nrwl/jest": "13.8.1",
    "@nrwl/node": "13.8.1",
    "@nrwl/nx-plugin": "13.8.1",
    "@nrwl/tao": "13.8.1",
    "@nrwl/workspace": "13.8.1",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@types/npm": "^2.0.31",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "aws-lambda": "^1.0.6",
    "dotenv": "10.0.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "jest-extended": "^0.11.5",
    "jest-mock-extended": "^1.0.9",
    "jsii": "^1.10.0",
    "jsii-pacmak": "^1.10.0",
    "jsonwebtoken": "^8.5.1",
    "npm": "^6.14.7",
    "prettier": "2.5.1",
    "rollup": "^2.23.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "27.0.5",
    "ts-node": "9.1.1",
    "tslib": "^2.0.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  }
}
