        }
    },
    "cli": {
        "defaultCollection": "@nrwl/nx-plugin"
    },
    "schematics": {
        "@nrwl/workspace": {
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
