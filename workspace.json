{
    "version": 1,
    "projects": {
        "aws-cdk": {
            "root": "packages/aws-cdk",
            "sourceRoot": "packages/aws-cdk/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": [
                            "packages/aws-cdk/tsconfig.lib.json",
                            "packages/aws-cdk/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!packages/aws-cdk/**/*"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/aws-cdk/jest.config.js",
                        "tsConfig": "packages/aws-cdk/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                },
                "build": {
                    "builder": "@nrwl/node:package",
                    "options": {
                        "outputPath": "dist/packages/aws-cdk",
                        "tsConfig": "packages/aws-cdk/tsconfig.lib.json",
                        "packageJson": "packages/aws-cdk/package.json",
                        "main": "packages/aws-cdk/src/index.ts",
                        "assets": [
                            "packages/aws-cdk/*.md",
                            {
                                "input": "./packages/aws-cdk/src",
                                "glob": "**/*.!(ts)",
                                "output": "./src"
                            },
                            {
                                "input": "./packages/aws-cdk",
                                "glob": "collection.json",
                                "output": "."
                            },
                            {
                                "input": "./packages/aws-cdk",
                                "glob": "builders.json",
                                "output": "."
                            }
                        ]
                    }
                }
            }
        },
        "aws-cdk-e2e": {
            "projectType": "application",
            "root": "e2e/aws-cdk-e2e",
            "sourceRoot": "e2e/aws-cdk-e2e/src",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/nx-plugin:e2e",
                    "options": {
                        "target": "aws-cdk:build",
                        "npmPackageName": "@nx-plug/aws-cdk",
                        "pluginOutputPath": "dist/packages/aws-cdk",
                        "jestConfig": "e2e/aws-cdk-e2e/jest.config.js",
                        "tsSpecConfig": "e2e/aws-cdk-e2e/tsconfig.spec.json"
                    }
                }
            }
        },
        "myin-cli": {
            "root": "packages/myin-cli",
            "sourceRoot": "packages/myin-cli/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": [
                            "packages/myin-cli/tsconfig.lib.json",
                            "packages/myin-cli/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!packages/myin-cli/**/*"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/myin-cli/jest.config.js",
                        "tsConfig": "packages/myin-cli/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                },
                "build": {
                    "builder": "@nrwl/node:package",
                    "options": {
                        "outputPath": "dist/packages/myin-cli",
                        "tsConfig": "packages/myin-cli/tsconfig.lib.json",
                        "packageJson": "packages/myin-cli/package.json",
                        "main": "packages/myin-cli/src/index.ts",
                        "assets": [
                            "packages/myin-cli/*.md",
                            {
                                "input": "./packages/myin-cli/src",
                                "glob": "**/*.!(ts)",
                                "output": "./src"
                            },
                            {
                                "input": "./packages/myin-cli",
                                "glob": "collection.json",
                                "output": "."
                            },
                            {
                                "input": "./packages/myin-cli",
                                "glob": "builders.json",
                                "output": "."
                            }
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/nx-plugin"
    },
    "schematics": {
        "@nrwl/workspace": {
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
                "linter": "eslint"
            }
        },
        "@nrwl/react": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/next": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/web": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/nx-plugin": {
            "plugin": {
                "linter": "eslint"
            }
        },
        "@nrwl/nest": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/express": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        }
    }
}
