{
  "version": 1,
  "projects": {
    "aws-cdk": {
      "root": "packages/aws-cdk",
      "sourceRoot": "packages/aws-cdk/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/aws-cdk/tsconfig.lib.json",
              "packages/aws-cdk/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!packages/aws-cdk/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/aws-cdk/jest.config.js",
            "tsConfig": "packages/aws-cdk/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/aws-cdk",
            "tsConfig": "packages/aws-cdk/tsconfig.lib.json",
            "packageJson": "packages/aws-cdk/package.json",
            "main": "packages/aws-cdk/src/index.ts",
            "assets": [
              "packages/aws-cdk/*.md",
              {
                "input": "./packages/aws-cdk/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/aws-cdk",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/aws-cdk",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "aws-cdk-e2e": {
      "projectType": "application",
      "root": "e2e/aws-cdk-e2e",
      "sourceRoot": "e2e/aws-cdk-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "aws-cdk:build",
            "npmPackageName": "@nx-plug/aws-cdk",
            "pluginOutputPath": "dist/packages/aws-cdk",
            "jestConfig": "e2e/aws-cdk-e2e/jest.config.js",
            "tsSpecConfig": "e2e/aws-cdk-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "aws-lambda": {
      "root": "packages/aws-lambda",
      "sourceRoot": "packages/aws-lambda/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/aws-lambda/tsconfig.lib.json",
              "packages/aws-lambda/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!packages/aws-lambda/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/aws-lambda/jest.config.js",
            "tsConfig": "packages/aws-lambda/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/aws-lambda",
            "tsConfig": "packages/aws-lambda/tsconfig.lib.json",
            "packageJson": "packages/aws-lambda/package.json",
            "main": "packages/aws-lambda/src/index.ts",
            "assets": [
              "packages/aws-lambda/*.md",
              {
                "input": "./packages/aws-lambda/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/aws-lambda",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/aws-lambda",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "aws-lambda-e2e": {
      "projectType": "application",
      "root": "e2e/aws-lambda-e2e",
      "sourceRoot": "e2e/aws-lambda-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "aws-lambda:build",
            "npmPackageName": "@nx-plug/aws-lambda",
            "pluginOutputPath": "dist/packages/aws-lambda",
            "jestConfig": "e2e/aws-lambda-e2e/jest.config.js",
            "tsSpecConfig": "e2e/aws-lambda-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "cdk-shared": {
      "root": "packages/cdk-shared",
      "sourceRoot": "packages/cdk-shared/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/cdk-shared/tsconfig.lib.json",
              "packages/cdk-shared/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!packages/cdk-shared/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/cdk-shared/jest.config.js",
            "tsConfig": "packages/cdk-shared/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}
